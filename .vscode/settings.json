{
    "vzfiletemplates.userTemplatesFolders": [
        "./_templates/"
    ],
    "vzfiletemplates.customVariablesConstructor": "./.vscode/customVars.cjs",
    "cSpell.enabledFileTypes": {
        "md": true,
    },
    "cSpell.language": "en,en-AU",
    "markdownlint.lintWorkspaceGlobs": [
        "**/*.{md,mkd,mdwn,mdown,markdown,markdn,mdtxt,mdtext,workbook}",
        "!**/*.code-search",
        "!**/bower_components",
        "!**/node_modules",
        "!**/.git",
        "!**/vendor",
        "**/*.mdx"
    ]
}